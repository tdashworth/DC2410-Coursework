{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 140],
    "no-console": false,
    "arrow-parens": false,
    "jsx-no-lambda": false
  }
}
